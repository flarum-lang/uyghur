{
	"name": "flarum-lang/uyghur",
	"type": "flarum-extension",
	"description": "Uyghur language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/uyghur",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"ug-locale",
		"uyghur-locale",
		"uyghur"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/uyghur/issues",
		"source": "https://github.com/flarum-lang/uyghur"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Uyghur",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "ug",
			"title": "Uyghur"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
